# Banco (obrigatório para white label completa)
DATABASE_URL=postgres://usuario:senha@localhost:5432/tracking

# Porta da API
PORT=4100

# Painel admin: criar projetos sem SQL (obrigatório em produção para acessar /painel)
ADMIN_SECRET=sua_chave_secreta_aqui

# URL base da API (usada no script e webhook exibidos no painel). Ex: https://track.ascensaodomentor.com
BASE_URL=

# Rate limit (requisições por minuto). Opcional.
# RATE_LIMIT_EVENTS_PER_MIN=120
# RATE_LIMIT_WEBHOOK_PER_MIN=60

# Meta: usado quando o projeto não tem integração em integrations_meta
META_PIXEL_ID=
META_ACCESS_TOKEN=
META_TEST_EVENT_CODE=
